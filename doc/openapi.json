          "phone": { "type": "string" }
        },
        "required": ["email", "info", "name", "phone"],
        "type": "object"
      },
      "Error": {
        "description": "Standard JSON error",
        "properties": {
          "message": { "type": "string" },
          "statusCode": { "maximum": 600, "minimum": 100, "type": "integer" }
        },
        "required": ["message", "statusCode"],
        "type": "object"
      }
    }
  },
